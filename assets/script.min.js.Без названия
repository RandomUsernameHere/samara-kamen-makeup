$(function(){
    $(".top-slider-outer").owlCarousel({
      items: 1,
      loop: !0,
      navigation: true,
      slideSpeed: 1000,
      navigationText: ["<div class='nav-btn prev-slide'><img src='/assets/template/img/icon/right-arrow.svg'/></div>","<div class='nav-btn next-slide'><img src='/assets/template/img/icon/right-arrow.svg'/></div>"]
    })
});

(function($) {
$(function() {
  
  $('ul._tabs-caption').on('click', 'li:not(.active)', function() {
    $(this)
      .addClass('active').siblings().removeClass('active')
      .closest('._tabs').find('._tabs-content').removeClass('active').eq($(this).index()).addClass('active');
  });
  
});
})(jQuery);

$(function(){
    $(".partners-slider").owlCarousel({
      items: 5,
      loop: !0,
      nav: !0,
      navText: [
        '<img src="img/icon/arrow-left.png/>"',
        '<img src="img/icon/arrow-right.png/>"'
      ]
    }),
    $(".reviews-slider").owlCarousel({ items: 2, center: !0 });
    
    var ops1 = $('#ops1');
    ops1.owlCarousel(
        {
            items: 1,
        }
    );
    
    $('a.control-slider-1').click(function(e){
        e.stopPropagation();
        var target = $(this).attr('href').slice(1);
        ops1.trigger('to.owl.carousel', [target, 300]);
    });
    
    var ops2 = $('#ops2');
    ops2.owlCarousel(
        {
            items: 1,
        }
    );
    
